{
  "extends": ["taro/react"],
  "rules": {
    // 配置忽略function前面的空格
    "space-before-function-paren": 0,
    // 关闭多余的空格
    "curly": [0, "all"],
    // 用于赋值结构
    "camelcase": [1, { "properties": "never" }],
    "no-unused-expressions": 0,
    "spaced-comment": 0,
    "react/no-direct-mutation-state": 0,
    "jsx-quotes": 0
  }
}
